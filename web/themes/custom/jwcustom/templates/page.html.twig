<div class ='header'>
    {{ page.header }}
    <nav class="header-navigation">
      <ul>
        <li><a href="/" data-drupal-link-system-path="&lt;front&gt;">Home</a></li>
        <li><a href="http://localhost:32782/node/2" title="SASS CSS ">SASS</a></li>
        <li><a href="#">Gallery</a></li>
        <li><a href="#">Contact</a></li>
      </ul>
    </nav>
    <img src="/sites/default/files/inline-images/drupal_0.png" />
</div>
<div class ='content'>

    {{ page.content }}
     <section class="bakery-header">
      <img src="/sites/default/files/inline-images/it-background.jpg" alt="Header Image for Bakery" />
    </section>
    <section class="gallery">
      <img src='./images/bakery-swirls.png' class="swirl"/>
      <div class="gallery-grid">
        <img src="/sites/default/files/inline-images/2toucons_0.jpg" />
        <img src="/sites/default/files/inline-images/jwimg.jpg" />
        <img src="/sites/default/files/inline-images/sassworking.png" />
      </div>

      <img src='./images/bakery-swirls.png' class="swirl upside-down"/>
    </section>
    <section class="about-us">
      <div class="about-us-text">
        <p>A site dedicated to my journey to understand Drupal and tools regularly used with it.</p>
      </div>
    </section>
</div>
<div class ='footer'>
    {{ page.footer }}
     <section class="footer-widgets">
      <div class="footer-logo">
        <img src="./images/lucky-star-bakery.jpg" />
      </div>
      <div class="contact-info">
        <div class="location">
          <p>123 Mulberry Lane</p>
          <p>Anytown, USA, 12345</p>
          <hr />
          <p>Phone: (555) 867-5309</p>
        </div>
      </div>
      <div class="contact-info">
        <div class="hours">
          <p><span class="hours-label">Sunday &amp; Monday:</span> CLOSED</p>
          <p><span class="hours-label">Tuesday - Friday:</span> 9:00am - 5:00pm</p>
          <p><span class="hours-label">Saturday:</span> 10:00am - 3:00pm</p>
        </div>
      </div>
    </section>
    <section class="copyright">
      <button class="btn-primary">Primary</button>
      <button class="btn-secondary">Secondary</button>
      <button class="btn-danger">Danger</button>
    </section>
</div>
